{
  "name": "uikit-react",
  "version": "2.1.0",
  "description": "UIkit components built with React",
  "main": "./lib",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "coveralls": "coveralls < .coverage/lcov.info",
    "prebuild": "rm -rf lib",
    "build": "babel ./src --out-dir ./lib",
    "prepublish": "npm run build",
    "storybook": "start-storybook -p 9001",
    "build-storybook": "build-storybook -o .out -s .storybook/static",
    "deploy-storybook": "storybook-to-ghpages",
    "postpublish": "gh-release && yarn run deploy-storybook",
    "postversion": "git add package.json && git commit -m \"chore(package): update version\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stipsan/uikit-react.git"
  },
  "keywords": [
    "react",
    "react-component",
    "uikit",
    "react-uikit",
    "ecosystem-react"
  ],
  "author": "Stian Didriksen <stipsan@gmail.com> (http://github.com/stipsan)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stipsan/uikit-react/issues"
  },
  "jest": {
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/lib/",
      "<rootDir>/node_modules/"
    ],
    "coverageDirectory": "<rootDir>/.coverage",
    "setupFiles": [
      "<rootDir>/test/setup.js"
    ]
  },
  "homepage": "https://uikit-react.io",
  "devDependencies": {
    "@kadira/react-storybook-addon-info": "3.2.2",
    "@kadira/storybook": "2.35.2",
    "@kadira/storybook-deployer": "1.2.0",
    "autoprefixer": "6.6.1",
    "babel-cli": "6.18.0",
    "babel-core": "6.21.0",
    "babel-eslint": "7.1.1",
    "babel-jest": "17.0.2",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-plugin-transform-react-inline-elements": "6.8.0",
    "babel-polyfill": "6.16.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "babel-register": "6.18.0",
    "coveralls": "2.11.15",
    "css-loader": "0.26.1",
    "eslint": "3.13.1",
    "eslint-config-airbnb": "13.0.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.9.0",
    "expect": "1.20.2",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "gh-release": "2.2.0",
    "growl": "1.9.2",
    "jest-cli": "18.1.0",
    "jsdom": "9.9.1",
    "json-loader": "0.5.4",
    "less": "2.7.2",
    "less-loader": "2.2.3",
    "mocha": "3.2.0",
    "postcss-loader": "1.2.1",
    "raw-loader": "^0.5.1",
    "react": "15.4.1",
    "react-addons-test-utils": "15.4.2",
    "react-collapse": "2.3.3",
    "react-dom": "15.4.1",
    "react-height": "2.1.1",
    "react-motion": "0.4.5",
    "react-test-renderer": "15.4.2",
    "style-loader": "0.13.1",
    "uikit": "2.27.2",
    "url-loader": "0.5.7"
  },
  "dependencies": {
    "classnames": "2.2.5",
    "react-input-autosize": "1.1.0",
    "react-portal": "^3.0.0"
  },
  "contributors": [
    {
      "name": "Suresh KUMAR Mukhiya",
      "email": "itsmeskm99@gmail.com",
      "url": "https://github.com/sureshHARDIYA"
    },
    {
      "name": "Torkil Johnsen",
      "email": "torkil@torkiljohnsen.com",
      "url": "https://github.com/torkiljohnsen"
    },
    {
      "name": "Zach Curtis",
      "email": "zachariahtimothy@gmail.com",
      "url": "https://github.com/zachariahtimothy"
    }
  ]
}
